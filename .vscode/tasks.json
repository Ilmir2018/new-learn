{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Open Brave DevTools",
			"type": "shell",
			"command": "brave",
			"args": [
				"brave://inspect/#devices"
			],
			"group": "none",
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": true,
				"clear": false
			}
		}
	]
}